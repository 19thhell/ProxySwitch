{
  "manifest_version": 2,
  "name": "Proxy List",
  "version": "1.0",

  "description": "This extension displays proxies.",

  "content_scripts": [{
	  "matches": ["http://www.google.com/*"],
	  "js": ["jquery-2.1.3.js", "popup.js"]
  }],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "author": "Imp",
  "permissions": [
	"proxy",
	"http://letushide.com/location/cn/"
  ]
}
